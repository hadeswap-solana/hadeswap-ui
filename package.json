{
  "name": "hadeswap-ui",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@sentry/react": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "@solana/wallet-adapter-base": "^0.9.9",
    "@solana/wallet-adapter-react": "^0.15.8",
    "@solana/wallet-adapter-wallets": "^0.16.8",
    "@tanstack/react-query": "^4.7.1",
    "antd": "^4.6.6",
    "classnames": "^2.3.1",
    "crypto-browserify": "^3.12.0",
    "hadeswap-sdk": "^0.3.4",
    "moment": "^2.29.1",
    "os-browserify": "^0.3.0",
    "ramda": "^0.28.0",
    "ramda-adjunct": "^3.1.0",
    "rc-tooltip": "^5.1.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.2.0",
    "react-confetti": "^6.1.0",
    "react-countup": "^6.1.0",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-hook-form": "^7.19.5",
    "react-infinite-scroll-component": "^6.1.0",
    "react-intersection-observer": "^9.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^5.2.0",
    "react-router-hash-link": "^2.4.3",
    "react-scripts": "^4.0.3",
    "react-select": "^5.2.1",
    "react-use-intercom": "^1.5.1",
    "redux": "^4.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.1.5",
    "sass": "^1.43.4",
    "socket.io-client": "^4.5.1",
    "stream-browserify": "^3.0.0",
    "swiper": "^7.4.1",
    "typed-scss-modules": "^6.5.0",
    "typesafe-actions": "^5.1.0",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --env SOLANA_NETWORK=devnet",
    "mainnet": "webpack serve --config webpack.dev.js --env SOLANA_NETWORK=mainnet",
    "bundle": "webpack build --config webpack.prod.js --env SOLANA_NETWORK=mainnet",
    "bundle-devnet": "webpack build --config webpack.prod.js --env SOLANA_NETWORK=devnet",
    "type-scss": "typed-scss-modules src --exportType default --nameFormat none",
    "build": "npm run type-scss && npm run bundle-devnet",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier-eslint --write $PWD/'**/*.{ts,tsx}'",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "analyze": "cross-env ANALYZE=1 GENERATE_SOURCEMAP=false npm run build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run format",
      "npm run lint"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/frakt-solana/hadeswap-ui"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@types/bn.js": "^4.11.6",
    "@types/ramda": "^0.28.13",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.1.2",
    "@types/react-router-dom": "^5.3.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-modules-typescript-loader": "^4.0.1",
    "dotenv": "^16.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.2",
    "less-loader": "^11.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "prettier-eslint": "^13.0.0",
    "prettier-eslint-cli": "^5.0.1",
    "react-refresh-typescript": "^2.0.7",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.4.3",
    "url-loader": "^4.1.1",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-dev-server": "^4.9.3"
  },
  "resolutions": {
    "**/@typescript-eslint/eslint-plugin": "^4.1.1",
    "**/@typescript-eslint/parser": "^4.1.1"
  }
}
